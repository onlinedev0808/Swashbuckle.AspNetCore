{
  "version": "6.0.0-beta9",
  "description": "Swagger Generator component for Swashbuckle - a Swagger toolchain for AspNet Core",
  "authors": [ "Richard Morris" ],

  "packOptions": {
    "tags": [ "Swagger Documentation Discovery Help WebApi AspNet AspNetCore" ],
    "projectUrl": "https://github.com/domaindrivendev/Swashbuckle",
    "licenseUrl": "https://github.com/domaindrivendev/Swashbuckle/LICENSE",
    "summary": "Provides a generator that can create accurate Swagger descriptions directly from your API implementation"
  },

  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Core": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Mvc.ApiExplorer": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Mvc.DataAnnotations": "1.0.0-rc2-final",
    "Swashbuckle.Swagger":  "6.0.0-beta9"
  },

  "frameworks": {
    "netstandard1.5": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "System.Xml.XPath": "4.0.1-rc2-24027"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    }
  }
}